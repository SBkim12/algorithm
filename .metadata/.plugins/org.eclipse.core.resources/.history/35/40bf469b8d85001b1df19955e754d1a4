import java.util.ArrayList;
import java.util.Collections;

public class 제일작은수제거하기 {

	public static void main(String[] args) {
		int[] arr = { 10, 5, 7, 1, 6, 5, 0, 1, 5, 3, 4, 8, 7 };
		int[] answer = {};
		if (arr.length == 1) {
			answer = new int[1];
			answer[0] = -1;
		} else {
			int min = 0;
			int index = 0;
			for (int i = 0; i < arr.length; i++) {
				if (min > arr[i]) {
					index = i;
					min = arr[i];
				}
			}

			ArrayList<Integer> modal = new ArrayList<>();

			for (int i = 0; i < arr.length; i++) {
				if (i == index) {
					modal.add(arr[i]);
				}
			}

			Collections.sort(modal, Collections.reverseOrder());

			answer = new int[modal.size()];
			for (int i = 0; i < modal.size(); i++) {
				answer[i] = modal.get(i);
				System.out.println(answer[i]);
			}
		}

	}

}
