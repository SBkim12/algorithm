
public class 소수구하기 {

	public static void main(String[] args) {
		int k=10;
		
		boolean flag;
		int cnt=0;
		for(int i=2; i<k; i++) {
			flag=true;
			for(int j=2; j<i; j++){
				if(i%j==0) {
					flag=false;
					break;
				}
			}
			if(flag==true) {
				System.out.println(i);
				cnt++;
			}
		}
		
		System.out.println(cnt);
	}

}
